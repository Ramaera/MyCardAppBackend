{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      "**/*.hbs"
    ],
    "deleteOutDir": true,
    "builder": "swc",
    "typeCheck": true,
    "plugins": ["@nestjs/swagger/plugin", "@nestjs/graphql/plugin"]
  }
}
